    "# Q2. Table Tennis\n",
    "![image-3.png](attachment:image-3.png)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 105,
   "id": "30375c63",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2 2\n",
      "1 2\n",
      "2\n"
     ]
    }
   ],
   "source": [
    "n, k_winnings = map(int, input().split())\n",
    "players = list(map(int, input().split()))\n",
    " \n",
    "#  3 cases to win\n",
    "# 1. a player wines the required times of rounds\n",
    "# 2. a player of the maximum power plays\n",
    "# 3. there is only two players\n",
    " \n",
    "maximum_power = max(players)\n",
    "winning_rounds = dict()\n",
    " \n",
    "def round_winner_has_maximum_power(player):\n",
    "    return round_winner == maximum_power\n",
    " \n",
    "def update_winnings_number(round_winner):\n",
    "    if round_winner not in winning_rounds.keys():\n",
    "            winning_rounds[round_winner] = 1\n",
    "    else:\n",
    "            winning_rounds[round_winner] += 1\n",
    "            \n",
    "def has_enough_winnings(round_winner):\n",
    "    return winning_rounds[round_winner] == k_winnings\n",
    " \n",
    "def remove_the_looser(round_looser):\n",
    "    players.remove(round_looser)\n",
    "    players.append(round_looser)\n",
    "    \n",
    "if len(players) == 2:                                  #the winner is the one who has the maximum power\n",
    "    WINNER = max(players[0],players[1])\n",
    "else:\n",
    "    while True:\n",
    "        round_winner = max(players[0],players[1])\n",
    " \n",
    "        if round_winner_has_maximum_power(round_winner):               #we found a winner\n",
    "            WINNER = round_winner\n",
    "            break\n",
    "        \n",
    "        update_winnings_number(round_winner)\n",
    "        \n",
    "        if has_enough_winnings(round_winner):  #we found a winner\n",
    "            WINNER = round_winner\n",
    "            break\n",
    " \n",
    "        remove_the_looser(min(players[0],players[1]))\n",
    "        \n",
    "print(WINNER)    "
   ]
  },

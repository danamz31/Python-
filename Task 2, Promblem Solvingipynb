    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 1 1\n",
      "2\n",
      "0\n"
     ]
    }
   ],
   "source": [
    "def replace_with_None(lst,split):\n",
    "    for i in range(len(lst)):    \n",
    "        if lst[i] > split:\n",
    "            lst[i] = None\n",
    "        \n",
    "number_of_prisoners, severe, transfered_prisoners = map(int, input().split())\n",
    "prisoners_severe = list(map(int, input().split()))\n",
    "\n",
    "copied_list = prisoners_severe.copy()\n",
    "\n",
    "replace_with_None(copied_list, severe)\n",
    "# print(copied_list)\n",
    "\n",
    "n = 0\n",
    "counter = 0\n",
    "for item in copied_list:\n",
    "    if item is not None:\n",
    "        n += 1\n",
    "    else:\n",
    "        n = 0\n",
    "    \n",
    "    if n >= transfered_prisoners:\n",
    "        counter += 1\n",
    "print(counter)"
   ]
